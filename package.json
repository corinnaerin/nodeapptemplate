{
  "name": "amazoninternaldashboard",
  "version": "1.0.0",
  "description": "Amazon Internal Dashboard",
  "scripts": {
    "postinstall": "npm run typings && npm run build",
    "typings": "tsd reinstall -k -o",
    "build": "node ./node_modules/gulp/bin/gulp.js release --verbose",
    "test": "node ./node_modules/gulp/bin/gulp.js test --verbose",
    "start": "ts-node server.ts"

  },
  "engines": {
    "node": ">= 4.4.4",
    "npm": ">= 1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-http": "^1.0.0",
    "express": "~4.13.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.6",
    "gulp-htmlmin": "^1.3.0",
    "gulp-mocha": "^2.1.3",
    "gulp-nodemon": "^2.0.6",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^2.0.0-alpha",
